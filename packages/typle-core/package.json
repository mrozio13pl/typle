{
  "name": "typle-core",
  "version": "0.2.0",
  "description": "Scan package.json for missing @types libraries.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    url: "mrozio13pl/typle",
    directory: "packages/typle-core"
  },
  "author": "mrozio13pl",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@npm/types": "^1.0.2",
    "find-up-keys": "^1.0.1",
    "resolve": "^1.22.8",
    "typle-util": "workspace:*"
  },
  "devDependencies": {
    "@types/resolve": "^1.20.6",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
